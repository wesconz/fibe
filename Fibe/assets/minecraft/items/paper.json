{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/paper"
    },
    "cases": [
      {
        "when": "shelf",
        "model": {
          "type": "model",
          "model": "custom/shelf/shelf"
        }
      },
      {
        "when": "toilet",
        "model": {
          "type": "model",
          "model": "custom/toilet"
        }
      },
      {
        "when": "pipe",
        "model": {
          "type": "model",
          "model": "custom/pipe"
        }
      },
      {
        "when": "bath",
        "model": {
          "type": "model",
          "model": "custom/bath/bath"
        }
      },
      {
        "when": "sink",
        "model": {
          "type": "model",
          "model": "custom/sink/sink"
        }
      },
      {
        "when": "curtain",
        "model": {
          "type": "model",
          "model": "custom/curtain"
        }
      },
      {
        "when": "sofa",
        "model": {
          "type": "model",
          "model": "custom/sofa"
        }
      }
    ]
  }
}